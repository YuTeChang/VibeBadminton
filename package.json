  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "npx tsx scripts/run-migration.ts",
    "start": "next start",
    "lint": "next lint",
    "test:screenshots": "node scripts/screenshot-test.js",
    "predeploy": "npm run lint && npm run build",
    "setup:vercel": "node scripts/setup-vercel.js",
